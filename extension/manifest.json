{
  "name": "Golinks",
  "description": "Golinks Chrome extension powered by CloudFlare Workers",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ]
}
